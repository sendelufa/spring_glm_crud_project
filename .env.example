# Database Configuration
DB_PASSWORD=your_secure_password_here

# Application Port
APP_PORT=8080

# Spring Profile (dev, test, prod)
SPRING_PROFILES_ACTIVE=prod

# JWT Secret (MUST BE 256+ bits - generate with: openssl rand -base64 32)
JWT_SECRET=your-super-secret-jwt-key-for-testing-change-this-in-production-at-least-256-bits

# JVM Options
JAVA_OPTS=-Xms256m -Xmx1g -XX:+UseG1GC
